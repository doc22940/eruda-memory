{
  "name": "eruda-memory",
  "version": "1.1.1",
  "description": "Eruda plugin for displaying memory",
  "main": "eruda-memory.js",
  "scripts": {
    "build": "uglifyjs eruda-memory.js -o eruda-memory.min.js --mangle",
    "lint": "eslint eruda-memory.js",
    "lint:fix": "npm run lint -- --fix",
    "ci": "npm run build && npm run lint",
    "dev": "nws -p 3000",
    "format": "prettier eruda-memory.js --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eruda-memory.git"
  },
  "keywords": [
    "eruda",
    "memory"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eruda-memory/issues"
  },
  "homepage": "https://github.com/liriliri/eruda-memory#readme",
  "devDependencies": {
    "eruda": "^1.5.8",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^6.1.0",
    "nws": "^1.1.1",
    "uglify-js": "^3.1.10"
  }
}
